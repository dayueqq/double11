<script type="text/javascript">
$(function(){
	var boxheight;  // 获得图片盒子的高度
	var boxwidth;  // 获得图片盒子的宽度
	<!--返回顶部-->
	$(window).scroll(function(){
		$(window).scrollTop()>400 ? $("#gotopbtn").fadeIn(1500).click(function(){
			$(window).scrollTop(0);
		}):$("#gotopbtn").fadeOut(1500);	
	});

	//在对这些缩略图的鼠标滑过事件
	$('.masonry_brick').live({  //使用live  可以对以后添加的div添加事件
		mouseenter:
		function(){
		   //显示标题
		   $(this).find('.box03').fadeIn(200);
		},
		mouseleave:
		function(){
		   //隐藏标题
		   $(this).find('.box03').fadeOut(200);
		}
	});
	
	//实现瀑布流的加载
	function item_callback(){ 
		$('.masonry_brick .group1').mouseover(function(){
			$(this).css('box-shadow', '0 2px 5px rgba(35,25,25,0.5)');
		}).mouseout(function(){
			$(this).css('box-shadow', '0 2px 3px rgba(34,25,25,0.2)');	 	
		});
		
		item_masonry();	

	}

	item_callback();  

	$('.masonry_brick').fadeIn();

	var sp = 1
	
	$(".infinite_scroll").infinitescroll({
		navSelector  	: "#more",
		nextSelector 	: "#more a",
		itemSelector 	: ".masonry_brick",
		
		loading:{
			img: "images/masonry_loading_1.gif",
			msgText: ' ',
			finishedMsg: '木有了',
			finished: function(){
				sp++;
				if(sp>=10){ //到第10页结束事件
					$("#more").remove();
					$("#infscr-loading").hide();
					$("#page").show();
					$(window).unbind('.infscr');
				}
			}	
		},errorCallback:function(){ 
			$("#page").show();
		}
		
	},function(newElements){
		var $newElems = $(newElements);
		$('.infinite_scroll').masonry('appended', $newElems, false);
		$newElems.fadeIn();
		item_callback();
		return;
	});

	<!--第四部分  更多分类-->
	$('#showbtn').mouseover(function(){
		$('#classic').animate({left:"0px"},function (){  
		      $('#showbtn').hide();
		      $('#closebtn').show();
		});
		$('#classic-cont').show();
	});
	$('#closebtn').mouseover(function(){
		$('#classic').animate({left:"-176px"},function () {    
			   $('#closebtn').hide();
		       $('#showbtn').show();
		});
	});
	
	<!--第五部分  搜索模块-->
	var timer=null;
	$('#searchPng').mouseover(function(){
		showSearch();
	});
	$('#searchPng').mouseout(function(){
	    timer=setTimeout(function(){
				disSearch();
		},2000);
    });
	$('#searchCont').click(function(){
		if(timer){
			clearTimeout(timer);
		};
	});
	$('#searchCont').mouseover(function(){
		if(timer){
			clearTimeout(timer);
		};
	});
	$('#searchCont').blur(function(){
	    timer=setTimeout(function(){
				disSearch();
		},2000);
    });

});

	<!-- 实现瀑布加载效果  -->
    function item_masonry(){ 
	    boxwidth=$('.masonry_brick').width();  // 获得图片盒子的宽度
		boxheight=(boxwidth*0.99);
		$('.masonry_brick').height(boxheight);
        boxgap=$('.demo').width()*0.03;
		$('.masonry_brick .group1 .box01 .box03').height($('.masonry_brick .group1 .box01').height()+10);
		//imgwidth=$('.group1 .box01. imgbox').width();  //设置图片的宽度
		//imgtop='-70px';  //设置图片的上边高度
		
		$('.imgbox img').load(function(){
			$('.infinite_scroll').masonry({ 
				itemSelector: '.masonry_brick',
				//columnWidth:160,
				gutterWidth:boxgap							
			});
			<!-- 改变图片 大小 -->
			//$(this).css('width',imgwidth);
			//$(this).css('marginTop',imgtop);
		});
			
		$('.infinite_scroll').masonry({ 
			itemSelector: '.masonry_brick',
			//columnWidth:160,
			gutterWidth:boxgap								
		});	
	}

	<!-- 显示搜索 -->
function showSearch(){
	$('#search').show(1000);
}
function disSearch(){
	$('#search').hide(1000);
}
</script>
